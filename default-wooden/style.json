{
    "name": "Wooden",
    "description": "The default style made from oak wood",
    "default": true,
    "file-type": ".nbt",

    "heads":
    [
        "buildings",
        "decorations"
    ],
    "registry": "$heads",

    "buildings":
    {
        "types":
        [
            "baker",
            "barracks",
            "barrackstower",
            "builder",
            "citizen",
            "composter",
            "cook",
            "cowboy",
            "deliveryman",
            "farmer",
            "fisherman",
            "guardtower",
            "chickenherder",
            "library",
            "lumberjack",
            "miner",
            "shepherd",
            "smeltery",
            "swineherder",
            "townhall",
            "warehouse"
        ],
        "registry": "$types",

        "any":
        {
            "files":
            [
                "$type 1",
                "$type 2",
                "$type 3",
                "$type 4",
                "$type 5"
            ],
            "registry": "$files"
        }
    },

    "decorations":
    {
        "types":
        [
            "camps",
            "fields",
            "infrastructureother",
            "infrastructureroads",
            "miner",
            "other",
            "parks",
            "walls"
        ],
        "registry": "$types",

        "camps":
        {
            "files":
            [
                "supplycamp",
                "supplyship"
            ],
            "registry": "$files"
        },
        "fields":
        {
            "files":
            [
                "simple",
                "fancy"
            ],
            "registry": "$files"
        },
        "infrastructureother":
        {
            "files":
            [
                "bridge",
                "bridgespan",
                "bridgestairs",
                "lamppostfancy",
                "lamppostnice",
                "lamppostpoor",
                "patrolpoint",
                "waypoint"
            ],
            "registry":
            [
                "bridge",
                "bridgespan",
                "bridgestairs",
                "lamppostfancy",
                "lamppostnice",
                "lamppostpoor"
            ]
        },
        "infrastructureroads":
        {
            "*":
            [
                "poor",
                "simple",
                "nice",
                "fancy"
            ],
            "files":
            [
                "*corner",
                "*intersection",
                "*path",
                "*stairs",
                "*tjunction"
            ],
            "registry": "$files"
        },
        "miner":
        {
            "files":
            [
                "mainshaft",
                "x2left",
                "x2right",
                "x2top",
                "x3leftright",
                "x3topleft",
                "x3topright",
                "x4"
            ],
            "registry": "$files"
        },
        "other":
        {
            "files":
            [
                "marketplace",
                "pillar",
                "simplefountain",
                "townlibrary",
                "well"
            ],
            "registry": "$files"
        },
        "parks":
        {
            "files":
            [
                "fancy",
                "simple"
            ],
            "registry": "$files"
        },
        "walls":
        {
            "files":
            [
                "corner",
                "gate",
                "stairs",
                "straight"
            ],
            "registry": "$files"
        }
    }
}